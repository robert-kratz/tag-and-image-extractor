{
  "manifest_version": 3,
  "name": "Tag-&-Image-Extractor",
  "version": "1.0",
  "description": "Extracts tags and images from a webpage.",
  "permissions": ["activeTab", "scripting", "downloads"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}